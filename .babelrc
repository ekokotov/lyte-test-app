{
  "presets": [
    ["@babel/preset-env",
      {
        "debug": true,
        "targets": {
          "chrome": "70",
          "firefox": "60",
          "edge": "16",
          "safari": "12"
        }
      }],
    ["@babel/preset-react"]
  ],
  "plugins": [
    "transform-react-remove-prop-types",
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }]
  ]
}
